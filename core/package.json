    "npm-run-all": "^4.1.5",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.25.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-live": "^2.3.0",
    "sass": "^1.42.1",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^2.2.4",
    "style-loader": "^2.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
